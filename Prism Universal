local ServerCrashButton = ExploitsTab:CreateButton({
   Name = "Server Crash (WARNING)",
   Callback = function()
       PRISM:Notify({
           Title = "Server Crash",
           Content = "Attempting to crash server... Use with caution!",
           Duration = 3,
           Image = 13047715178
       })
